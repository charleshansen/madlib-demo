<%= form_for :house do |f| %>
    Tax: <%= f.text_field :tax %><br />
    Bedroom : <%= f.text_field :bedroom %><br />
    Bath : <%= f.text_field :bath %><br />
    Price : <%= f.text_field :price %><br />
    Size    : <%= f.text_field :size %><br />
    Lot    : <%= f.text_field :lot %><br />
    <%= f.submit 'Add House'%>
<% end %>